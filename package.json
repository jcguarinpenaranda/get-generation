{
  "name": "get-generation",
  "version": "1.2.3",
  "description": "Gets the generation of the population, given a birth year",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run snyk-protect; npm run build",
    "test": "npx jest --maxWorkers=2",
    "snyk-protect": "snyk protect"
  },
  "keywords": [
    "get",
    "generation",
    "by",
    "year"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jcguarinpenaranda/get-generation.git"
  },
  "author": "Juan Camilo Guarin P",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^2.7.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\.test\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@types/jest": "^23.3.2",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.3",
    "snyk": "^1.189.0"
  },
  "snyk": true
}
