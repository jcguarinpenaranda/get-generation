{
  "name": "get-generation",
  "version": "1.1.4",
  "description": "Gets the generation of the population, given a birth year",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "test": "npx jest --maxWorkers=2 --no-coverage"
  },
  "keywords": [
    "get",
    "generation",
    "by",
    "year"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jcguarinpenaranda/get-generation.git"
  },
  "author": "Juan Camilo Guarin P",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.1",
    "typescript": "^2.7.2"
  },
  "jest": {
    "mapCoverage": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\.test\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
